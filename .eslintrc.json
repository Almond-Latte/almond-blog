{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": [],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 13
  },
  "parser": "@typescript-eslint/parser",
  "env": { "browser": true },
  "globals": {},
  "rules": {
    "semi": "error",
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off"
  }
}
